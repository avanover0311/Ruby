<div class="container">
      <p><% flash.each do |key, value|  %>
    <div class="alert alert-<%= key %>" role="alert">
        <p><%= value %> </p>
      </p>
    </div>
      <% end %>
    
  <nav class="jumbotron">
    <a class="navbar-brand" href="/users/"style="color:#CC5E23">Fernweh Expeditions</a>
  </nav>
    <div class="row">
      <div class="col-sm-2 register">
        <h2>Register</h2>
          <form action = "/users" method = "post">
            <input type = 'hidden' name = "authenticity_token" value = "<%=form_authenticity_token%>">
              <label>First Name</label>
                <input type = "text" name = "user[first_name]" id = "first_name">
              <label>Last Name</label>
                <input type = "text" name = "user[last_name]" id = "last_name">
              <label>Email</label>
                <input type = "text" name = "user[email]" id = "email">
              <label>Password</label>
                <input type = "password" name = "user[password]" id = "password">
              <label> Confirm Password</label>
                <input type = "password" name = "user[password_confirmation]" id = "password_confirm">
                  <input type = "submit" value = "Register">
          </form>
      </div>
        <img src="https://images.unsplash.com/photo-1535437239777-81c9aa19d4e1?ixlib=rb-0.3.5&ixid=eyJhcHBfaWQiOjEyMDd9&s=6a65084ea90c9aedfba61a9d3520c362&auto=format&fit=crop&w=634&q=80" class="col-lg-6 imgregister">
    </div>
</p>
</div>
</div>